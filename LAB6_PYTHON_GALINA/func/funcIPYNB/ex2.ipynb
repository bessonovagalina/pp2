    "str1 = str(input(\"Enter the string: \"))\n",
    "\n",
    "def AZ(char):\n",
    "    return ord('A') <= ord(char) <= ord('Z')\n",
    "\n",
    "def az(char):\n",
    "    return ord('a') <= ord(char) <= ord('z')\n",
    "\n",
    "bigl = list(filter(AZ, str1))\n",
    "smalll = list(filter(az, str1))\n",
    "\n",
    "print(len(bigl), len(smalll))\n"
